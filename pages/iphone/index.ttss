.items{
	font-size: 30rpx;
	display: flex;
	padding: 18rpx;
	justify-content: space-between;
	height: 55rpx;
}
.items-title{
	line-height: 55rpx;
}
.items-title image{
	width: 60rpx;
	height: 60rpx;
	border-radius: 50%;
}
.bottoma{
	width: 98vw;
	margin: 0 auto;
	height: 2rpx;
	background: #CCCCCC;
}
.items-image image{
	width: 20rpx;
	height: 20rpx;
	position: relative;
	top: 12rpx;
}
.items-titlea{
	flex: 1;
	text-align: right;
	line-height: 55rpx;
	margin-right: 10rpx;
	color: #999999;
}
.classname{
	width: 80vw;
	height: 70rpx;
	line-height: 70rpx;
	color: #ffffff;
	font-size: 30rpx;
	background: #FF9900;
	border-radius: 10rpx;
	margin: 0 auto;
	position: relative;
	top: 100rpx;
	text-align: center;
}